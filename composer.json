{
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-amqp": "*",
        "ext-json": "*",
        "beberlei/assert": "^3.3",
        "broadway/broadway-bundle": "^0.6.1",
        "broadway/event-store-dbal": "^0.6",
        "doctrine/doctrine-bundle": "^2.8",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.14",
        "elasticsearch/elasticsearch": "^7.11",
        "lexik/jwt-authentication-bundle": "^2.11",
        "nelmio/api-doc-bundle": "^4.1|4.x-dev",
        "ramsey/uuid": "^4.1",
        "ramsey/uuid-doctrine": "^1.6",
        "symfony/apache-pack": "^1.0",
        "symfony/amqp-messenger": "^6.3",
        "symfony/asset": "^6.3",
        "symfony/cache": "^6.3",
        "symfony/console": "^6.3",
        "symfony/flex": "^1.12",
        "symfony/framework-bundle": "^6.3",
        "symfony/messenger": "^6.3",
        "symfony/monolog-bundle": "^3.6",
        "symfony/proxy-manager-bridge": "^6.3",
        "symfony/security-bundle": "^6.3",
        "symfony/twig-bundle": "^6.3",
        "symfony/yaml": "^6.3"
    },
    "require-dev": {
        "blackfire/php-sdk": "^1.24",
        "dama/doctrine-test-bundle": "^6.5",
        "phpstan/phpstan": "^1.4.6",
        "phpunit/phpunit": "v9.3",
        "rector/rector": "^0.15.13",
        "roave/security-advisories": "dev-master",
        "sylius-labs/coding-standard": "^v4.2.0",
        "symfony/browser-kit": "^6.3",
        "symfony/css-selector": "^6.3",
        "symfony/dotenv": "^6.3",
        "symfony/phpunit-bridge": "^6.3",
        "symfony/stopwatch": "^6.3",
        "symfony/web-profiler-bundle": "^6.3",
        "vimeo/psalm": "^4.20"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/flex": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/App/",
            "UI\\": "src/UI/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-iconv": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "id": "01C1QKYZ4DSB74RHER7JCF8Q1K",
            "allow-contrib": false,
            "require": "^6.3"
        }
    },
    "name": "jorge07/symfony-5-es-cqrs-boilerplate",
    "description": "Symfony 5 DDD ES CQRS backend boilerplate"
}
